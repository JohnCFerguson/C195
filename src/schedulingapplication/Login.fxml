    <children>
        <Button fx:id="signInBtn" layoutX="128.0" layoutY="161.0" onAction="#signIn" text="Sign In" />
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <TextField fx:id="userNameText" layoutX="128.0" layoutY="45.0" />
      <TextField fx:id="passwordText" layoutX="128.0" layoutY="88.0" />
      <Label fx:id="usernameLbl" layoutX="58.0" layoutY="49.0" text="Username" />
      <Label fx:id="passwordLbl" layoutX="58.0" layoutY="92.0" text="Password" />
      <Label fx:id="invalidLogin" layoutX="63.0" layoutY="14.0" prefHeight="17.0" prefWidth="196.0" />
    </children>
</AnchorPane>
